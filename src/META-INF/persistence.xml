    <class>pos_project.classes.Stocks</class>
    <class>pos_project.classes.Transfer</class>
    <class>pos_project.classes.Retruns</class>
    <class>pos_project.classes.Returns</class>
    <class>pos_project.classes.ReturnMain</class>
    <class>pos_project.classes.PreorderTransaction</class>
    <class>pos_project.classes.Preorder</class>
    <class>pos_project.classes.ItemLimit</class>
    <class>pos_project.classes.OtReportAccess</class>
    <class>pos_project.classes.ArReportAccess</class>
    <class>pos_project.classes.UserTypeAccess</class>
    <class>pos_project.classes.Convert</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/pos_db"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
